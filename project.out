SQL> -- Function for find the amount of incidents for a Model
SQL> CREATE OR REPLACE FUNCTION modelIncidentNums(model IN Model.vehicleModel%TYPE, make IN Model.vehicleMake%TYPE)
  2  	 RETURN INTEGER IS
  3  	 num INTEGER;
  4  BEGIN
  5  	 SELECT COUNT(*) INTO num
  6  	   FROM vehicle v, incident i
  7  	   WHERE v.numLicensePlate = i.numLicensePlate AND
  8  		 v.plateState = i.plateState AND
  9  		 v.vehicleModel = model AND
 10  		 v.vehicleMake = make;
 11  	 RETURN num;
 12  END modelIncidentNums;
