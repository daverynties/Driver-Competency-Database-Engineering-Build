SQL> -- Function for finding the date of last incident for a vehicle
SQL> CREATE OR REPLACE FUNCTION dateLastIncident(nLicensePlate IN Vehicle.numLicensePlate%TYPE, pState IN Vehicle.plateState%TYPE)
  2  	 RETURN DATE IS
  3  	 lastdate DATE;
  4  BEGIN
  5  	 SELECT DISTINCT i.reportedDate INTO lastDate
  6  	   FROM incident i
  7  	   WHERE i.numLicensePlate = nLicensePlate AND
  8  		 i.plateState = pState
  9  	   ORDER BY i.reportedDate DESC
 10  	   FETCH FIRST ROW ONLY;
 11  --
 12  	 RETURN lastDate;
 13  END dateLastIncident;
