SQL> CREATE OR REPLACE TRIGGER dric4
  2  	 BEFORE INSERT OR UPDATE ON Drives
  3  	 FOR EACH ROW
  4  DECLARE
  5  	 PRAGMA AUTONOMOUS_TRANSACTION;
  6  	 numNotMatch INTEGER;
  7  	 IDS INTEGER;
  8  BEGIN
  9  	 SELECT groupID INTO IDS
 10  	     FROM Driver
 11  	     WHERE userID = :NEW.userID;
 12  
 13  	 SELECT COUNT(*) INTO numNotMatch
 14  	     FROM Driver d, Drives dr
 15  	     WHERE d.userID = dr.userID AND
 16  		   dr.numLicensePlate = :NEW.numLicensePlate AND
 17  		   dr.plateState = :NEW.plateState AND
 18  		   dr.userID <> :NEW.userID AND
 19  		   d.groupID <> IDS;
 20  
 21  	 IF numNotMatch > 0
 22  	 THEN
 23  	     RAISE_APPLICATION_ERROR(-20001, '++++Insert or Update rejected. '||'User ID ' ||:NEW.userID|| ' part of different Group ID.');
 24  	 END IF;
 25  END;
 26  --
 27  /

